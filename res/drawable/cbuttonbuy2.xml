<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <bitmap android:gravity="center" android:src="@drawable/baseline_monetization_on_white_36" />
            </item>
            <item>
                <shape android:shape="oval"
                  xmlns:android="http://schemas.android.com/apk/res/android">
                    <gradient android:startColor="#33696969" android:endColor="#33f3f3f3" android:angle="0.0" android:type="radial" android:gradientRadius="50.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_focused="true">
        <layer-list>
            <item>
                <bitmap android:gravity="center" android:src="@drawable/baseline_monetization_on_white_36" />
            </item>
            <item>
                <shape android:shape="oval"
                  xmlns:android="http://schemas.android.com/apk/res/android">
                    <gradient android:startColor="#33696969" android:endColor="#33b3b3b3" android:angle="0.0" android:type="radial" android:gradientRadius="50.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_enabled="true">
        <layer-list>
            <item>
                <bitmap android:gravity="center" android:src="@drawable/baseline_monetization_on_white_36" />
            </item>
        </layer-list>
    </item>
</selector>
